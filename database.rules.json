{
  /* Visit https://firebase.google.com/docs/database/security to learn more about security rules. */
  "rules": {
    ".read": false,
    ".write": false,
    "users": {
    	".indexOn": "email",
      "date": {
            ".validate": "newData.val() === now"
       }
  	},
    "profile": {
    	".indexOn": "handle",
      "date": {
            ".validate": "newData.val() === now"
       }
  	}
  }
  
}